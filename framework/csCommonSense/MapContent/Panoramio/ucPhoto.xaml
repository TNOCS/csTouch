<UserControl x:Class="csGeoLayers.Content.Panoramio.UcPhoto"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" x:Name="surfaceUserControl"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" xmlns:csGeoLayers="clr-namespace:csGeoLayers" mc:Ignorable="d">
	
    <Border Margin="-15,-15,0,0" BorderBrush="Black" BorderThickness="1" Width="30" Height="30">
        <Viewbox Stretch="UniformToFill">
        <csGeoLayers:ucDocument Document="{Binding Document, ElementName=surfaceUserControl }" /> 
        </Viewbox>
        <!--<Image x:Name="iPhoto" Stretch="UniformToFill" />-->
    
	</Border>
</UserControl>
